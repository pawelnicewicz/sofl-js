<!-- Contact -->
<section id="contact">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading text-uppercase">Contact Us</h2>
        <h3 class="section-subheading text-muted">Don't be shy. Just do it!</h3>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <form id="contactForm" name="contactForm" method="post" action="/courseRequest">
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <input class="form-control" id="name" name="name" type="text" placeholder="Your Name *" required="required" data-validation-required-message="Please enter your name.">
                <p class="help-block text-danger"></p>
              </div>
              <div class="form-group">
                <input class="form-control" id="email" name="email" type="email" placeholder="Your Email *" required="required" data-validation-required-message="Please enter your email address.">
                <p class="help-block text-danger"></p>
              </div>
              <div class="form-group">
                <input class="form-control" id="phone" name="phone" type="tel" placeholder="Your Phone *" required="required" data-validation-required-message="Please enter your phone number.">
                <p class="help-block text-danger"></p>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <textarea class="form-control" id="message" name="message" placeholder="Your Message *" required="required" data-validation-required-message="Please enter a message."></textarea>
                <p class="help-block text-danger"></p>
              </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-lg-12 text-center">
              <div id="success"></div>
              <button id="sendMessageButton" class="btn btn-primary btn-xl text-uppercase" type="submit">Send Message</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
  <div class="container-fluid">
    <div style="width: 100%">
      <iframe width="100%" height="450" 
      src="https://maps.google.com/maps?width=100%&amp;height=450&amp;hl=en&amp;q=Sienna%2039%2C%20Warsaw+(My%20Business%20Name)&amp;ie=UTF8&amp;t=&amp;z=17&amp;iwloc=B&amp;output=embed" 
      frameborder="0" scrolling="no" marginheight="0" marginwidth="0">
        <a href="https://www.maps.ie/map-my-route/">Draw map route</a>
      </iframe>
    </div><br />
  </div>
</section>

<!-- Careers -->
<section class="bg-light" id="careers">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2 class="section-heading text-uppercase">Careers</h2>
        <h3 class="section-subheading" id="text-careers">Looking for a job in education? <br>
        If you're an experienced lector of a foreign language, be part of our team. Come on, give us a go!</h3>
      </div>
    </div>
    <div class="col-lg-12 text-center">
      <form name="getCareers" action="/careers">
        <button id="sendMessageButton" class="btn btn-primary btn-xl text-uppercase btn-careers" type="submit">Join our Team!</button>
      </form>
    </div>
  </div>
</section>

